  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:contract": "jest --testPathPattern=src/__tests__/capture-services/knowledge-ingestion.contract.test.ts",
    "clean": "rm -rf dist",
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node src/main.ts",
    "dev:watch": "ts-node-dev --respawn --transpile-only src/main.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "gen:types": "openapi-typescript specifitions/api-documentation/backend/knowledge-ingestion-api.yaml --output src/shared/types/knowledge-ingestion.ts",
